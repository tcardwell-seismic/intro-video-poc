{
    "name": "intro-video-poc",
    "version": "0.0.1",
    "description": "",
    "main": "index.js",
    "private": true,
    "scripts": {
        "start": "npm run start:standalone",
        "start:seismic-embed": "webpack serve --open --mode development --config ./config/webpack.seismicEmbed.js",
        "build": "shx rm -rf dist/seismic && webpack --mode production --config ./config/webpack.seismicEmbed.js",
        "start:standalone": "concurrently --kill-others-on-fail \"npm run server\" \"npm run start:standalone-app\" ",
        "start:standalone-app": "webpack serve --open --hot --mode development --config ./config/webpack.standalone.js",
        "build:standalone": "shx rm -rf dist/standalone && webpack --mode production --config ./config/webpack.standalone.js",
        "test": "jest --coverage",
        "lint": "eslint \"./src/**/*.{js,jsx,ts,tsx}\"",
        "lint:fix": "eslint './src/**/*.{js,jsx,ts,tsx}' --fix",
        "stylelint": "npx stylelint \"**/*.scss\"",
        "prepush": "npm run test -- --watchAll=false && npm run lint && npm run stylelint",
        "server": "ts-node-dev --project ./proxy-server/tsconfig.json ./proxy-server/server.ts"
    },
    "husky": {
        "hooks": {
            "pre-push": "npm run prepush"
        }
    },
    "keywords": [],
    "author": "",
    "license": "UNLICENSED",
    "devDependencies": {
        "@axe-core/react": "^4.1.1",
        "@babel/core": "^7.13.15",
        "@babel/plugin-transform-react-jsx": "^7.13.12",
        "@babel/plugin-transform-typescript": "^7.13.0",
        "@babel/preset-env": "^7.13.15",
        "@seismic/browser-utils": "1.0.0",
        "@seismic/toolchain": "^2.3.0",
        "@testing-library/jest-dom": "^5.11.10",
        "@testing-library/react": "^11.2.6",
        "@types/angular": "^1.8.1",
        "@types/express": "^4.17.11",
        "@types/jest": "^26.0.22",
        "@types/react": "^17.0.3",
        "@types/react-dom": "^17.0.3",
        "@types/react-router-dom": "^5.1.7",
        "@types/react-transition-group": "^4.4.1",
        "@types/styled-components": "^5.1.9",
        "@types/testing-library__jest-dom": "^5.9.5",
        "@typescript-eslint/eslint-plugin": "^4.21.0",
        "@typescript-eslint/parser": "^4.21.0",
        "axios": "^0.21.1",
        "babel-jest": "^26.6.3",
        "babel-loader": "^8.2.2",
        "babel-plugin-transform-class-properties": "^6.24.1",
