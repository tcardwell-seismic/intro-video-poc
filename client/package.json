        "react": "16.14.0",
        "react-dom": "16.14.0",
        "react-router-dom": "^5.2.0",
        "seismic-common": "0.0.16-develop.134"
    },
    "jest": {
        "roots": [
            "<rootDir>/src",
            "<rootDir>/config"
        ],
        "collectCoverageFrom": [
            "src/**/*.{js,jsx,ts,tsx}",
            "!src/**/*.d.ts"
        ],
        "testMatch": [
            "<rootDir>/src/**/*.test.{js,jsx,ts,tsx}"
        ],
        "transform": {
            "^.+\\.(js|jsx|ts|tsx)$": "babel-jest",
            "^.+\\.(css|scss|sass)$": "<rootDir>/config/jestCSSTransform.js",
            "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/config/jestFileTransform.js"
        },
        "transformIgnorePatterns": [
            "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
            "^.+\\.module\\.(css|scss|sass)$"
        ],
        "moduleNameMapper": {
            "^src/(.*)$": "<rootDir>/src/$1",
            "^.+\\.module\\.(css|scss|sass)$": "identity-obj-proxy"
        }
    },
    "babel": {
        "presets": [
            [
                "babel-preset-react-app",
                {
                    "typescript": true
                }
            ]
        ]
    }
}
